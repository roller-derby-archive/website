<table class="rda-ftfr-table">
    <thead>
    <tr>
        <th>Rang</th>
        <th></th>
        <th>Èquipe</th>
        <th>Note</th>
        <th>Rang europe</th>
    </tr>
    </thead>
    <tbody>
    {% for teamRank in frenchRanking %}
        <tr id="{{ teamRank.id }}">
            <td class="rda-ftfr-rank-cell">{{ teamRank.frenchRank }}</td>
            <td class="rda-ftfr-logo-cell">
                {% if teamRank.team.logo.name != null %}
                    {{ include('widget/_logo.twig', {hasLogoEntity: teamRank.team}) }}
                {% else %}
                    <img alt="logo-team" src=" https://placehold.co/36x36">
                {% endif %}
            </td>
            <td class="rda-ftfr-name-cell">
                <a class="rda-wiki-link" href="{{ path('team_view', {id: teamRank.team.id}) }}">{{ teamName(teamRank.team.name, teamRank.team.pronoun) }}</a>
                <p>
                    {% if teamRank.team.level != null %}<span class="rda-team-level">{{ teamRank.team.level }}</span>{% endif %}
                    {% if teamRank.team.type != 'A' %}<span class="rda-team-type">{{ teamType(teamRank.team.type) }}</span>{% endif %}
                </p>
            </td>
            <td class="rda-ftfr-other-cell">⚝{{ teamRank.rating}}</td>
            <td class="rda-ftfr-other-cell">{{ teamRank.europeanRank}}{% if teamRank.europeanRank == 1 %}er{% else %}ème{% endif %}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>
